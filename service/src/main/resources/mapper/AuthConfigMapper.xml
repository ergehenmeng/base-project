<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eghm.mapper.AuthConfigMapper">

    <select id="getByAppKey" resultType="com.eghm.vo.auth.AuthConfigVO">
        select app_key, public_key, private_key, expire_date from auth_config where app_key = #{appKey} limit 1
    </select>
</mapper>
