<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eghm.mapper.PoiAreaMapper">

    <select id="getList" resultType="com.eghm.vo.poi.PoiAreaResponse">
