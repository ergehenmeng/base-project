<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eghm.mapper.SysAreaMapper">

    <select id="getList" resultType="com.eghm.vo.sys.SysAreaVO">
        select
        id,
        title,
        zip_code,
        mark,
        pid
        from sys_area
